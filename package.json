{
    "main": "src/app/boot.ts",
    "scripts": {
        "start": "npm run start:dev",
        "start:dev": "nodemon -r tsconfig-paths/register --transpile-only",
        "start:prod": "node build/api/boot.js",
        "seed": "npx knex seed:run",
        "migrate": "npx knex migrate:latest --knexfile knexfile.ts",
        "migrate:fresh": "npx knex migrate:down --knexfile knexfile.ts && npm run migrate"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^5.1.3",
        "@types/node": "^16.10.2",
        "dayjs": "^1.10.7",
        "dotenv-cli": "^4.0.0",
        "nodemon": "^2.0.13",
        "pg": "^8.7.1",
        "ts-node": "^10.2.1",
        "tsconfig-paths": "^3.11.0",
        "typeorm": "^0.2.37",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.4.0",
        "inversify": "^5.1.1",
        "inversify-express-utils": "^6.3.2",
        "knex": "^0.95.11",
        "objection": "^2.2.17",
        "reflect-metadata": "^0.1.13"
    }
}
